{
  "name": "slate-mdast-serializer",
  "version": "0.1.1",
  "description": "serialize slate to mdast and vice versa",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run prebuild && npm run build",
    "prebuild": "rimraf lib",
    "build": "babel src --out-dir lib",
    "dev": "babel -w src --out-dir lib",
    "test": "babel-tape-runner src/**/*.test.js | faucet",
    "tape": "babel-tape-runner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbiting/mdast.git"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/orbiting/mdast/issues"
  },
  "homepage": "https://github.com/orbiting/mdast#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-tape-runner": "^2.0.1",
    "faucet": "0.0.1",
    "immutable": "^3.8.2",
    "rimraf": "^2.6.2",
    "slate": "^0.30.6",
    "tape": "^4.8.0"
  },
  "peerDependencies": {
    "slate": "^0.30.6"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "remark-frontmatter": "^1.1.0",
    "remark-parse": "^4.0.0",
    "remark-stringify": "^4.0.0",
    "unified": "^6.1.5"
  }
}
